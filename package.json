{
  "name": "dz-glob",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.4.0",
  "description": "",
  "author": "Daniel Zhang",
  "license": "MIT",
  "homepage": "https://github.com/danielzhang183/dz-glob#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielzhang183/dz-glob.git"
  },
  "bugs": "https://github.com/danielzhang183/dz-glob/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "play": "vite playground",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.16.3",
    "bumpp": "^8.2.1",
    "eslint": "^8.39.0",
    "esno": "^0.16.3",
    "pnpm": "^8.4.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5",
    "unbuild": "^0.8.11",
    "vite": "^4.3.4",
    "vitest": "^0.23.4"
  },
  "dependencies": {
    "fast-glob": "^3.2.12",
    "magic-string": "^0.30.0"
  }
}
